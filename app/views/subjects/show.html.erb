<%= stylesheet_link_tag 'style.css' %>
<%= link_to("BACK TO LIST", subjects_path, :class => 'back-link')%>
<div class="subjects show">
	<h1>Show Subjects</h1>
	<table summary="Subject detail view">
		<tr>
			<th>Name</th>
			<td><%= @subjects.name %></td>
		</tr>
		<tr>
			<th>Position</th>
			<td><%= @subjects.visible ? 'true' : 'false' %></td>
		</tr>
		<tr>
			<th>Created</th>
			<td><%= @subjects.created_at %></td>
		</tr>
		<tr>
			<th>Updated</th>
			<td><%= @subjects.updated_at %></td>
		</tr>
	</table>
</div>